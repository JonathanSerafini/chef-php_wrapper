include=<%= "#{node[:php][:fpm_pooldir]}/*.conf" %>

[global]
pid = <%= node[:php][:fpm_pid] %>
error_log = <%= node[:php][:fpm_error_log] %>
log_level = <%= node[:php][:fpm_log_level] %>

emergency_restart_threshold = <%= node[:php][:fpm_restart_threshold]  %>
emergency_restart_interval = <%= node[:php][:fpm_restart_interval]  %>
process_control_timeout = <%= node[:php][:fpm_control_timeout]  %>

daemonize = yes
