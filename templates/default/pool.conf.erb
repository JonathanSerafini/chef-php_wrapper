; Generated by Chef
; Local modifications will be overwritten.
;

[<%= @name %>]
listen = <%= @listen %>
listen.backlog = <%= @backlog %>
<% if @listen.slice(0) == '/' %>
listen.owner = <%= @listen_user %>
listen.group = <%= @listen_group %>
listen.mode = <%= @listen_mode %>
<% end %>
<% unless @allowed_clients.empty? %>
listen.allowed_clients = <%= [@allowed_clients].flatten.join "," %>
<% end %>

user = <%= @user %>
group = <%= @group %>

pm = <%= @process_manager %>
pm.process_idle_timeout = <%= @process_idle_timeout %>
pm.max_requests = <%= @max_requests %>
pm.max_children = <%= @max_children %>
<% if @process_manager == "dynamic" %>
pm.start_servers = <%= @start_servers %>
pm.min_spare_servers = <%= @min_spare_servers %>
pm.max_spare_servers = <%= @max_spare_servers %>
<% end %>
<% unless @status_path.nil? %>
pm.status_path = <%= @status_path %>
<% end %>
<% unless @ping_path.nil? %>
ping.path = <%= @ping_path %>
<% end %>

request_terminate_timeout = <%= @request_terminate_timeout %>

catch_workers_output = <%= @catch_workers_output %>
<% unless @security_limit_extensions.empty? %>
security.limit_extensions = <%= [@security_limit_extensions].flatten.join " " %>

<% end %>
<% @env_variables.each do |k,v| %>
env[<%= k %>] = <%= v %>
<% end %>
<% @php_variables.each do |k,v| %>
php_value[<%= k %>] = <%= v %>
<% end %>
<% @php_flags.each do |k,v| %>
php_flag[<%= k %>] = <%= v %>
<% end %>
<% @php_admin_variables.each do |k,v| %>
php_admin_value[<%= k %>] = <%= v %>
<% end %>
<% @php_admin_flags.each do |k,v| %>
php_admin_flag[<%= k %>] = <%= v %>
<% end %>
